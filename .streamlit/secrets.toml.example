# Streamlit Secrets for Deployment
# Copy this to secrets.toml and add your actual values

[secrets]
OPENAI_API_KEY = "your_openai_api_key_here"
TAVILY_API_KEY = "your_tavily_api_key_here"
SALLA_ACCESS_TOKEN = "your_salla_access_token_here"
SALLA_REFRESH_TOKEN = "your_refresh_token_here"
SALLA_CLIENT_ID = "your_client_id_here"
SALLA_CLIENT_SECRET = "your_client_secret_here"